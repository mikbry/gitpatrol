{
  "compilerOptions": {
    "types": [],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "composite": true,
    "plugins": [],
    "outDir": "dist",
    "noEmit": true,
    "baseUrl": ".",
    "rootDir": "."
  },
  "exclude": ["**/*.test.ts"]
}